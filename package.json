{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --mode staging",
    "build-development": "vue-cli-service build --mode development",
    "build-staging": "vue-cli-service build --mode staging",
    "build-staging-test": "vue-cli-service build --mode staging-test --no-unsafe-inline",
    "build-production": "vue-cli-service build --mode production --no-unsafe-inline",
    "serve": "vue-cli-service serve --mode staging",
    "serve-development-node": "DEBUG=express:* VUE_APP_MODE=development node index.js",
    "serve-staging": "vue-cli-service serve --mode staging",
    "serve-staging-node": "DEBUG=express:* VUE_APP_MODE=staging node index.js",
    "serve-staging-test": "sudo DEBUG=express:* VUE_APP_MODE=staging-test node index.js 2>&1 | tee \"./logs/deployment_$(date +'%d_%m__%H_%M').log\" &",
    "serve-production": "sudo DEBUG=express:* VUE_APP_MODE=production node index.js 2>&1 | tee \"./logs/deployment_$(date +'%d_%m__%H_%M').log\" &",
    "lint": "vue-cli-service lint",
    "apollo": "vue-cli-service apollo:dev --generate-schema",
    "apollo:schema:generate": "vue-cli-service apollo:schema:generate",
    "apollo:schema:publish": "vue-cli-service apollo:schema:publish",
    "apollo:start": "vue-cli-service apollo:start"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^4.4.2",
    "@fullcalendar/interaction": "^4.4.2",
    "@fullcalendar/list": "^4.4.2",
    "@fullcalendar/timegrid": "^4.4.2",
    "@fullcalendar/vue": "^4.4.2",
    "ant-design-vue": "^1.6.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "auth0-js": "^9.13.2",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "chart.js": "^2.9.3",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.3",
    "graphql-type-json": "^0.2.1",
    "ics": "^2.22.1",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.26.0",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-ldapauth": "^2.1.4",
    "rxjs": "^6.6.2",
    "shortid": "^2.2.8",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.3",
    "vue-chartjs": "^3.5.0",
    "vue-js-modal": "^2.0.0-rc.3",
    "vue-router": "^3.3.2",
    "vue-rx": "^6.2.0",
    "vue-sidebar-menu": "^4.5.4",
    "vuejs-clipper": "^3.0.1",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-alpha.5",
    "graphql-tag": "^2.9.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "sass-loader": "^10.0.1",
    "vue-cli-plugin-apollo": "~0.21.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
